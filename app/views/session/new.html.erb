<strong> <h1> Sign in to create a movie review! </h1> </strong>

<hr>


<%= form_tag signin_path do |f| %>

<div>
<%= label_tag :username, "Username" %>
<%= text_field_tag :username %>
</div>

<div>
<%= label_tag :password, "Password" %>
<%= password_field_tag :password %>
</div>

<div>
   <%= submit_tag "Sign In" %>
</div>

<% end %>
